{
  "workbench.colorTheme": "Shades of Purple",
  "workbench.iconTheme": "vscode-icons",
  "editor.wordWrap": "bounded",
  "prettier.jsxBracketSameLine": true,
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "git.enableSmartCommit": true,
  "extensions.autoUpdate": false,
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "gitProjectManager.displayProjectPath": true,
  "gitProjectManager.warnIfFolderNotFound": true,
  "vetur.trace.server": "messages",
  "files.associations": {
      "*.vue": "vue",
      "tslint.json": "jsonc",
      "eslint.json": "jsonc"
  },
  "[vue]": {},
  "[javascript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.options": { "extensions": [".html", ".js", ".vue", ".jsx"] },
  "eslint.validate": [
      "javascript",
      "javascriptreact",
      "vue",
      "typescript",
      "typescriptreact"
  ],
  "vetur.validation.template": false,
  "vetur.experimental.templateInterpolationService": true,
  "vetur.format.defaultFormatter.html": "prettier",
  "vetur.format.defaultFormatter.scss": "prettier",
  "vetur.format.defaultFormatter.css": "prettier",
  "vetur.format.defaultFormatter.js": "prettier",
  "javascript.validate.enable": true,
  "javascript.format.enable": true,
  "editor.codeActionsOnSave": {
      "source.fixAll.tslint": true,
      "source.fixAll.eslint": true
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[typescript]": {
      "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[shellscript]": {
      "editor.defaultFormatter": "shakram02.bash-beautify"
  },
  "git.confirmSync": false,
  "git.autofetch": true,
  "workbench.startupEditor": "newUntitledFile",
  "go.formatTool": "goimports",
  "go.useLanguageServer": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "go.lintTool": "golangci-lint",
  "eslint.alwaysShowStatus": true,
  "eslint.codeAction.showDocumentation": {
      "enable": true
  },
  "prettier.tabWidth": 4,
  "workbench.panel.defaultLocation": "right",
  "go.toolsManagement.autoUpdate": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.fontFamily": "'cascadia code', Consolas, 'Courier New', monospace",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorWidth": 3,
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "debug.allowBreakpointsEverywhere": true,
  "debug.console.closeOnEnd": true,
  "terminal.integrated.copyOnSelection": true,
  "redhat.telemetry.enabled": true,
  "terminal.integrated.fontFamily": "'cascadia code'",
  "security.workspace.trust.untrustedFiles": "open",
  "editor.tabSize": 2,
  "window.zoomLevel": 2,
  "sonarlint.ls.javaHome": "/usr",
  "terminal.integrated.defaultLocation": "editor",
  "sonarlint.pathToNodeExecutable": "/home/yusuf/.nvm/versions/node/v16.14.0/bin/node",
  "go.alternateTools": {
      "go": "/snap/go/current/bin/go"
  },
  "[go]": {
      "editor.insertSpaces": true,
      "editor.formatOnSave": true,
      "editor.defaultFormatter": "golang.go",
      "editor.codeActionsOnSave": {
          "source.organizeImports": true
      },
      "editor.suggest.snippetsPreventQuickSuggestions": false
  }
}
